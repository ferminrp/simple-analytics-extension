{
  "manifest_version": 2,

  "name": "Simple Analytics Extension",
  "description": "Track your simple analytics dashboards.",
  "version": "0.0.0.1",

  "icons": {
    "48": "SimpleAnalytic.png"
  },
  "content_security_policy": "script-src 'self' https://simpleanalytics.com 'unsafe-eval'; object-src 'self'",
  "permissions": [
    "activeTab",
    "https://*/"
  ],
  "background": { "scripts": ["jquery.js","background.js"] },
  "browser_action": {
    "default_icon": "SimpleAnalytic.png",
    "default_popup": "popup.html"
  }
}
